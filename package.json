{
  "name": "edge-core-js",
  "version": "0.19.3",
  "description": "Edge account & wallet management library",
  "keywords": [
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "homepage": "https://edge.app",
  "repository": {
    "type": "git",
    "url": "git@github.com:EdgeApp/edge-core-js.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "William Swanson <william@edge.app>",
    "Tim Horton <tim@edge.app>",
    "Paul Puey <paul@edge.app>",
    "Eliran Zach <eliran@edge.app>",
    "Kevin Sullivan <kevin@edge.app>",
    "Allen Hujsak <allen@edge.app>"
  ],
  "exports": {
    ".": {
      "import": "./lib/node/index.mjs",
      "require": "./lib/node/index.js"
    },
    "./package.json": "./package.json",
    "./types": {
      "import": "./types.mjs",
      "require": "./types.js"
    }
  },
  "main": "./lib/node/index.js",
  "module": "./lib/index.js",
  "browser": "./lib/browser.js",
  "types": "./lib/exports.d.ts",
  "files": [
    "/android/build.gradle",
    "/android/src/*",
    "/CHANGELOG.md",
    "/edge-core-js.podspec",
    "/ios/*",
    "/lib/*",
    "/LICENSE",
    "/package.json",
    "/README.md",
    "/src/types/*",
    "/types.d.ts",
    "/types.js.flow",
    "/types.js",
    "/types.mjs"
  ],
  "scripts": {
    "build.lib": "sucrase ./src -q -d ./lib -t flow,jsx",
    "build.node": "rollup -c",
    "build.react-native": "webpack",
    "build.types": "node -r sucrase/register ./scripts/make-types.js && tsc",
    "clean": "rimraf lib",
    "fix.android": "(cd android; ./format-java.sh)",
    "fix.ios": "swift-format format -i ios/*.swift",
    "start": "webpack serve",
    "fix": "npm run lint -- --fix",
    "flow": "flow",
    "lint": "eslint .",
    "precommit": "lint-staged && flow && npm-run-all test prepare",
    "prepare": "husky install && npm-run-all clean -p build.*",
    "prepare.dev": "EDGE_MODE=development npm-run-all clean -p build.*",
    "test": "mocha -r sucrase/register 'test/**/*.test.js'",
    "verify": "flow && npm-run-all lint prepare test"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "dependencies": {
    "aes-js": "^3.1.0",
    "base-x": "^1.0.4",
    "biggystring": "^3.0.2",
    "cleaners": "^0.3.11",
    "currency-codes": "^1.1.2",
    "disklet": "^0.4.5",
    "edge-sync-client": "^0.2.6",
    "elliptic": "^6.4.0",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "hash.js": "^1.1.7",
    "hmac-drbg": "^1.0.1",
    "node-fetch": "^3.1.1",
    "redux": "^3.6.0",
    "redux-keto": "^0.3.4",
    "redux-pixies": "^0.3.6",
    "rfc4648": "^1.4.0",
    "scrypt-js": "^2.0.3",
    "serverlet": "^0.1.0",
    "yaob": "^0.3.7",
    "yavent": "^0.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@sucrase/webpack-loader": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "assert": "^2.0.0",
    "assert-log": "^0.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-fake-error-class": "^1.0.0",
    "buffer": "^6.0.3",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-kit": "0.15.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "flow-bin": "^0.89.0",
    "husky": "^7.0.0",
    "lint-staged": "^10.2.11",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-flow-entry": "^0.3.6",
    "rollup-plugin-mjs-entry": "^0.1.1",
    "stream-browserify": "^3.0.0",
    "sucrase": "^3.20.0",
    "typescript": "^4.1.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "react-native": "lib/react-native.js"
}
