{
  "name": "edge-core-js",
  "version": "0.17.12",
  "description": "Edge account & wallet management library",
  "keywords": [
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "homepage": "https://edge.app",
  "repository": {
    "type": "git",
    "url": "git@github.com:EdgeApp/edge-core-js.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "William Swanson <william@edge.app>",
    "Tim Horton <tim@edge.app>",
    "Paul Puey <paul@edge.app>",
    "Eliran Zach <eliran@edge.app>",
    "Kevin Sullivan <kevin@edge.app>",
    "Allen Hujsak <allen@edge.app>"
  ],
  "main": "./index.js",
  "module": "./lib/index.js",
  "browser": "./lib/browser.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/CHANGELOG.md",
    "/index.js.flow",
    "/index.js",
    "/lib/*",
    "/LICENSE",
    "/package.json",
    "/README.md",
    "/src/types/*",
    "/types.js.flow",
    "/types.js"
  ],
  "scripts": {
    "build.lib": "sucrase ./src -q -d ./lib -t flow,jsx",
    "build.react-native": "webpack",
    "build.types": "node ./make-types.js; tsc",
    "clean": "rimraf lib",
    "fix": "npm run lint -- --fix",
    "flow": "flow",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "precommit": "lint-staged && flow && npm test && npm run prepare",
    "prepare": "npm-run-all clean -p build.*",
    "test": "mocha -r sucrase/register 'test/**/*.test.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "dependencies": {
    "aes-js": "^3.1.0",
    "base-x": "^1.0.4",
    "biggystring": "^3.0.2",
    "cleaners": "^0.2.1",
    "currency-codes": "^1.1.2",
    "disklet": "^0.4.5",
    "elliptic": "^6.4.0",
    "esm": "^3.2.4",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "hash.js": "^1.1.7",
    "hmac-drbg": "^1.0.1",
    "json-csv": "^1.5.0",
    "node-fetch": "^2.0.0",
    "redux": "^3.6.0",
    "redux-keto": "^0.3.4",
    "redux-pixies": "^0.3.6",
    "rfc4648": "^1.4.0",
    "scrypt-js": "^2.0.3",
    "yaob": "^0.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "assert-log": "^0.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-fake-error-class": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "eslint-config-standard-kit": "^0.14.4",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.89.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sucrase": "^3.15.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "react-native": "lib/react-native.js"
}
